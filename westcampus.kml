            <color>f0ffed42</color>
        </LineStyle>
        <PolyStyle>
            <color>e8ff9257</color>
        </PolyStyle>
      <BalloonStyle>
        <text>HIIII</text>
      </BalloonStyle>
    </Style>
    <Placemark>
